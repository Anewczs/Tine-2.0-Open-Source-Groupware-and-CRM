<?xml version="1.0" encoding="UTF-8"?>
<config>
    <rules>
        <rule>
            <field>adr_one_postalcode</field>
            <regex>/\s*(\d{5})\s+[a-zA-Z]+/m</regex>
        </rule>
        <rule>
            <field>adr_one_postalcode</field>
            <regex>/[A-Z]{1,3}-(\d{5})\s+[a-zA-Z]+/m</regex>
        </rule>
        <rule>
            <field>adr_one_locality</field>
            <regex>/\d{5}\s+([a-zA-Z ]+)/m</regex>
        </rule>
        <rule>
            <field>adr_one_street</field>
            <regex>/^([a-zA-Z]+\s+\d+)/m</regex>
        </rule>
        <rule>
            <field>org_name</field>
            <regex>/(^.*(?:GmbH|GbR|OHG|KG).*)/m</regex>
        </rule>
        <rule>
            <field>tel_work</field>
            <regex>/^(?:Tel[^+^0-9]*)(\+{0,1}[0-9\ \t()-]+)/m</regex>
        </rule>
        <rule>
            <field>tel_fax</field>
            <regex>/^(?:Fax[^+^0-9]*)(\+{0,1}[0-9\ \t()-]+)/m</regex>
        </rule>
        <rule>
            <field>email</field>
            <regex>/([a-z0-9_\+-\.]+@[a-z0-9-\.]+\.[a-z]{2,5})/i</regex>
        </rule>
        <rule>
            <field>url</field>
            <regex>/((mailto\:|(news|(ht|f)tp(s?))\:\/\/){1}\S+)/i</regex>
        </rule>
    </rules>
</config>
